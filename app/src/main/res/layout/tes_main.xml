<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_test"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="RxHttp 框架测试"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <com.eeepay.zzq.rxhttpdemo.ItemEditText
        android:id="@+id/iet_input"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:labelText="开户名称"
        app:editText="庄泽钦"
        app:hintText=""
        app:isEdit="false"
        app:isNumber="false"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/btn_test"
        />
    <Button
        android:id="@+id/btn_test2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="RxHttp 框架测试"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toBottomOf="@id/btn_test"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <com.eeepay.zzq.rxhttpdemo.ItemEditText
        android:id="@+id/iet_input2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:labelText="开户名称"
        app:editText="庄泽钦"
        app:hintText=""
        app:isEdit="false"
        app:isNumber="false"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toBottomOf="@id/iet_input"
        app:layout_constraintLeft_toRightOf="@id/btn_test2"
        />
</androidx.constraintlayout.widget.ConstraintLayout>